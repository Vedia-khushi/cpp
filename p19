using namespace std;
#include<iostream>

using namespace std;
int main()
{
	char operation;
	int num1,num2;
	
	
	cout<<"Vediya Khushi R.";
	cout<<"220130318010";
	
	cout<<"Enter the number1 :: ";
	cin>>num1;
	cout<<"Enter the number2 :: ";
	cin>>num2;
	
	switch(operation)
	{
		case '+':
		cout<<"Result"<<num1+num2;
		break;
		
		case '-':
		cout<<"Result"<<num1-num2;
		break;
		
		case '*':
		cout<<"Result"<<num1*num2;
		break;
	
	
		case '/':
		cout<<"Result"<<num1/num2;
		break;
		
		case '%':
		cout<<"Result"<<num1%num2;
		break;
		
		default:
			cout<<"invalid";	
	
	}
	return 0; 
}
