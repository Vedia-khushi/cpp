#include<iostream>

using namespace std;

int fabonaaci(int x)
{
	int i,f1=0,f2=1,f3=0;
	
	cout<<f1;
	cout<<f2;
	
	for(i=0;i<x;i++)
	{
		f3=f1+f2;
		f1=f2;
		f2=f3;
		
		cout<<f3;
	}
	
}
int main()
{
	int num;
	
	cout<<"Vediya Khushi R.";
	cout<<"220130318010";
	
	cout<<"Enter the number :: ";
	cin>>num;
	fabonaaci(num);
	
	return 0;
}
