#include<iostream>
using namespace std;

int prime(int x)
{
	int i;
	
	for(i=0;i<x;i++)
	{
		if(x%1==0)
		{
			return false;
		}
			return true;
	}
}

int main()
{
	int n;
	
	cout<<"Vediya Khushi R."<<endl;
	cout<<"220130318010"<<endl;
	
	cout<<"Enter the number ::";
	cin>>n;
	if(prime(n))
	{
		cout<<"The number is prime"<<endl;
	}
	else
	{
		cout<<"The number is not prime"<<endl;
	}
	
		return 0;
}
	
