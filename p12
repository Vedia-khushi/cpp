#include <iostream>
using namespace std;

int main()
 {

    int n;
    
    cout<<"Vediya Khushi R."<<endl;
	cout<<"220130318010"<<endl;

    cout << "Enter a positive integer: ";
    cin >> n;

    
    for (int i = 1; i <= 10; i++) {
        cout << n << " * " << i << " = " << n * i << endl;
    }
    
    return 0;
}

